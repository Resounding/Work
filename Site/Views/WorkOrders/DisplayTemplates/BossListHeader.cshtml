@model Site.Models.BossListViewModel

<header>
    <div class="crew-day">&nbsp;</div>
    @foreach (var crew in Model.CrewNames) {
        <div class="crew-day"><h2>@crew</h2></div>
    }
    <div class="clearfix"></div>
    <div class="toolbar">
        <a href="@Url.Action("New")" class="button new" data-intro="This is where you can add new Work Orders" data-position="right"><i class="icon-plus"></i> New Work Order</a>
    </div>
</header>